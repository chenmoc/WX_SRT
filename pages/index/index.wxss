.container{
  width: 100%;
  height: 100vh;
  background-color: #ffffff;
}

/* .nav-header{
  display: flex;
  justify-content: center;
} */

.nav-head{
  margin-top: 20rpx;
  height: 407rpx;
  width: 660rpx;
  /* background-color: #233D5C; */
  border-radius: 30rpx;
}

.nav-img{
  height: 407rpx;
  width: 660rpx;
  border-radius: 30rpx;
}

.nav-list {
  display: flex;
  flex-wrap: wrap;
  padding: 0rpx 40rpx 0rpx;
  justify-content: space-between;
  margin-top: 50rpx;
  /* height: 100vh; */
}

.nav-li {
  padding: 30rpx;
  border-radius: 30rpx;
  width: 45%;
  margin: 0 2.5% 40rpx;
  color: white;
  position: relative;
  z-index: 1;
}

.nav-title {
  text-align: right;
  font-size: 35rpx;
  font-weight: 300;
}

.nav-name {
  position: absolute;
  margin:auto;
  right: 30rpx;
  font-size: 30rpx;
  text-align: right;
  text-transform: Capitalize;
  position: relative;
}

.nav-li text {
  margin-top: 20rpx;
  position: absolute;
  font-size: 52rpx;
  text-align: left;
  line-height: 60rpx;
}

